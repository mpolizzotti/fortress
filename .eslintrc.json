{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true,
        "jasmine": true,
        "node": true
    },
    "globals": {
        "angular": false,
        "jquery": false,
        "describe": false,
        "fdescribe": false,
        "xdescribe": false,
        "it": false,
        "fit": false,
        "xit": false,
        "expect": false,
        "beforeEach": false,
        "afterEach": false,
        "inject": false,
        "spyOn": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}